@model RewardsForYou.Models.Users

@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>

<div class="jumbotron">
    <h1>Rewards For You</h1>
    <p class="lead">Sign in your account</p>
    <form>
        <div class="form-group">
            <label for="username">Username</label>
            <asp:TextBox type="username" ID="TxtUsername" runat="" class="form-control" id="username" placeholder="Enter Username"></asp:TextBox>
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Password</label>
            <asp:TextBox type="password" class="form-control" id="exampleInputPassword1" placeholder="Enter Password"></asp:TextBox>
        </div>
        <button type="submit" ID="TxtUsername" runat="" class="btn btn-primary">Sign in</button>
    </form>

</div>

